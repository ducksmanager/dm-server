{
    "name": "bperel/dm-server",
    "license": "Apache-2.0",
    "description": "DucksManager server REST API endpoint",
    "type": "library",
    "keywords": ["ducksmanager", "whattheduck"],
    "homepage": "https://ducksmanager.net",
    "authors": [
        {
            "name": "Bruno Perel",
            "homepage": "https://github.com/bperel"
        }
    ],
    "support": {
        "issues": "https://github.com/bperel/dm-server/issues"
    },
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-json": "*",
        "ext-intl": "*",
        "beberlei/doctrineextensions": "^1.2",
        "cloudinary/cloudinary_php": "^1.14",
        "pusher/pusher-push-notifications": "^1.1",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony-bundles/json-request-bundle": "^3.0",
        "symfony/console": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/flex": "^1.4",
        "symfony/framework-bundle": "^5.1",
        "symfony/maker-bundle": "^1.12",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^2.0",
        "symfony/security-bundle": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^5.1",
        "symfony/twig-bundle": "^5.1",
        "symfony/validator": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "liip/test-fixtures-bundle": "^1.3.0",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^5.1",
        "symfony/css-selector": "^5.1",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/stopwatch": "^5.1",
        "symfony/web-profiler-bundle": "^5.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
